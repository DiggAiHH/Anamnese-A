{
  "name": "praxis-code-generator",
  "version": "1.0.0",
  "description": "Practice code generator for digital anamnesis with Stripe payment integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-basic.js",
    "test:codec": "node test-anamnese-codec.js",
    "test:handoff": "node test-handoff-store.js",
    "test:audit": "node test-audit-utils.js",
    "test:all": "npm test && npm run test:codec && npm run test:handoff && npm run test:audit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup": "node setup.js",
    "lighthouse": "mkdir -p reports && npx lighthouse http://localhost:8080/index_v8_complete.html --output=html --output-path=reports/lighthouse.html --only-categories=performance,accessibility,best-practices,seo --quiet --chrome-flags='--headless=new'"
  },
  "keywords": [
    "medical",
    "anamnesis",
    "practice",
    "code-generator",
    "stripe",
    "payment"
  ],
  "author": "DiggAi GmbH",
  "license": "PROPRIETARY",
  "dependencies": {
    "@fontsource/roboto": "^5.2.9",
    "bcryptjs": "^3.0.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.3",
    "pdfjs-dist": "^5.4.530",
    "pg": "^8.11.3",
    "python": "^0.0.4",
    "qrcode": "^1.5.4",
    "stripe": "^14.10.0",
    "tesseract.js": "^7.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@types/express": "^4.17.25",
    "@types/node": "^25.0.3",
    "axe-playwright": "^2.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
